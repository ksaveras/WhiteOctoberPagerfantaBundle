{
    "name": "white-october/pagerfanta-bundle",
    "description": "Bundle to use Pagerfanta with Symfony2",
    "keywords": ["page","paging"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo DÃ­ez",
            "email": "pablodip@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "pagerfanta/pagerfanta": "^1.1.0|^2.0.0",
        "symfony/framework-bundle": "~4.3|~5.0",
        "symfony/property-access": "~4.3|~5.0",
        "symfony/translation": "~4.3|~5.0",
        "symfony/twig-bundle": "~4.3|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "symfony/symfony": "4.3"
    },
    "conflict": {
        "twig/twig": "<1.34|>=2.0,<2.4"
    },
    "autoload": {
        "psr-4": {
            "WhiteOctober\\PagerfantaBundle\\": ""
        },
        "exclude-from-classmap": ["Tests/", "TestsProject/"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
